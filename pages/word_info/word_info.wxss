/* 编辑修改按钮的配置 */
#edit_button_view {
    width: 90vw;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-top: 50px;
}
#edit_button {
    margin-right: 20px;
    margin-bottom: 5px;
}
#edit_button button {
    font-size: 13px !important;
    font-weight: normal;
    padding: 2px 15px 2px 15px !important;
}
#delete_button {
    margin-left: 20px;
    margin-bottom: 5px;
}
#delete_button button {
    font-size: 13px !important;
    font-weight: normal;
    padding: 2px 15px 2px 15px !important;
}
